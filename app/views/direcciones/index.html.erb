<h1>Listing direcciones</h1>

<table>
  <tr>
    <th>Descripcion</th>
    <th>Calle</th>
    <th>Numero</th>
    <th>Comentarios</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @direcciones.each do |direccion| %>
  <tr>
    <td><%= direccion.descripcion %></td>
    <td><%= direccion.calle %></td>
    <td><%= direccion.numero %></td>
    <td><%= direccion.comentarios %></td>
    <td><%= link_to 'Show', direccion %></td>
    <td><%= link_to 'Edit', edit_direccion_path(direccion) %></td>
    <td><%= link_to 'Destroy', direccion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New direccion', new_direccion_path %>
